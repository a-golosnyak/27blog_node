{
   "name": "typeorm",
   "version": "0.0.0",
   "private": true,
   "engines": {
      "node": "^18"
   },
   "scripts": {
      "start": "ts-node src/app.ts",
      "dev": "nodemon --exec ts-node src/app.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "fresh": "ts-node src/database/fresh.ts",
      "seed": "ts-node src/database/seeds",
      "test": "NODE_ENV=testing jest --forceExit --detectOpenHandles  --silent"
   },
   "dependencies": {
      "bcryptjs": "2.4.3",
      "chai-http": "4.3.0",
      "cookie-parser": "1.4.4",
      "debug": "2.6.9",
      "express": "5.1.0",
      "jsonwebtoken": "9.0.2",
      "mongodb": "6.17.0",
      "morgan": "~1.9.1",
      "nodemon": "2.0.15",
      "reflect-metadata": "0.2.2",
      "typeorm": "0.3.25",
      "validatorjs": "3.22.1"
   },
   "devDependencies": {
      "@faker-js/faker": "6.0.0-beta.0",
      "@types/express": "5.0.3",
      "@types/jest": "27.5.0",
      "@types/jsonwebtoken": "^9.0.10",
      "@types/mongodb": "4.0.7",
      "@types/node": "16.11.10",
      "@types/validatorjs": "3.15.5",
      "chai": "4.3.6",
      "faker": "5.5.3",
      "jest": "28.1.0",
      "mocha": "10.0.0",
      "ts-jest": "28.0.2",
      "ts-node": "10.9.2",
      "typescript": "4.5.2"
   },
   "jest": {
      "verbose": true,
      "testMatch": [
         "**/src/tests/**/*.ts"
      ],
      "roots": [
         "<rootDir>/src/tests"
      ],
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "module": "esnext",
      "target": "es2017"
   }
}
